page {
  background-color: #F7F8F8;
}
.all{
  width: 750rpx;
  height: auto;
  margin: 0 auto;
  position: relative;
  margin-bottom: 120rpx;
}
.flex{
  display: flex;
  justify-content: center;
  align-items: center;
}
.commission_container{
  width: 100%;
  height: 347rpx;
  background: #0090FF;
}
.info_container{
  height: 150rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.info_child_container1{
  width: 220rpx;
  margin-left: 16rpx;
}
.info_child_container1>view:first-of-type{
  font-size: 26rpx;
  color: #FFF;
  line-height: 30rpx;
  text-align: center;
}
.info_child_container1>view:last-of-type{
  font-size: 28rpx;
  color: #FFF;
  line-height: 34rpx;
  margin-top: 10rpx;
  text-align: center;
}
.info_container .line{
  height: 71rpx;
  width: 1rpx;
  background: rgba(180,180,181,0.4);
}
.info_container .details{
  position: absolute;
  right: 32rpx;
  font-size: 26rpx;
  color: #FFF;
  line-height: 30rpx;
}
.amount_total{
  font-size: 54rpx;
  color: #FFF;
  line-height: 64rpx;
  font-weight: 500;
  margin-top: 20rpx;
  text-align: center;
}
.commission_title{
  font-size: 26rpx;
  color: #FFF;
  line-height: 30rpx;
  margin-top: 24rpx;
  text-align: center;
}
.common_container{
  width: 694rpx;
  border-radius:17rpx;
  background: #FFF;
}
.common_container>view{
  width: 100%;
  height: 83rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.common_container>view>image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 35rpx;
}
.common_container>view .title{
  font-size: 26rpx;
  color: #9FA0A0;
  position: absolute;
  left: 90rpx;
}
.common_container>view .amount{
  font-size: 26rpx;
  position: absolute;
  right: 36rpx;
  color: #9FA0A0;
}
.bottom_border{
  border-bottom: 1rpx solid rgba(180,180,181,0.4);
}
.title_container{
  width: 100%;
  height: 84rpx;
  display: flex;
  align-items: center;
}
.title_container>text{
  font-size: 28rpx;
  color: #231815;
  margin-left: 36rpx;
}
.desc_content{
  min-height: 126rpx;
}
.desc_content>text{
  font-size: 26rpx;
  color: #9FA0A0;
  line-height: 35rpx;
  padding: 0 36rpx;
}
.pay_conatiner{
  width: 100%;
  display: flex;
  flex-direction: row;
}
.pay_conatiner .wechat{
  width: 50%;
  height: 154rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.pay_conatiner .alipay{
  width: 50%;
  display: flex;
  align-items: center;
  position: relative;
}
.wechat>view{
  position: absolute;
  left: 96rpx;
  width: 208rpx;
  height: 69rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 17rpx;
  box-sizing: border-box;
}
.alipay>view{
  position: absolute;
  right: 96rpx;
  width: 208rpx;
  height: 69rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 17rpx;
  box-sizing: border-box;
}
.alipay>view>image,.wechat>view>image{
  width: 33rpx;
  height: 33rpx;
}
.alipay>view>text,.wechat>view>text{
  margin-left: 7rpx;
  font-size: 26rpx;
  color: #231815;
  line-height: 33rpx;
}
.select{
  border:1rpx solid #008FFF;
}
.unselect{
  border:1rpx solid rgba(0,0,0,0.4);
}


.submit-box{
  width: 100%;
  height: 98rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 4;
  background: #FFF;
  display: flex;
  flex-direction: row;
  justify-content: center;
}   
.submit-info{
  width:750rpx;
  height:100%;
  display: flex;
  background: #0090FF;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  color: #FFF;
}





