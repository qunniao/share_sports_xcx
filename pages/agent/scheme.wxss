page {
  background-color: #F7F8F8;
}
.all{
  width: 750rpx;
  height: auto;
  margin: 0 auto;
  position: relative;
}
.filter_container{
  display: flex;
  flex-direction: row;
  min-height: 40rpx; 
  margin-left: 56rpx;
  margin-bottom: 30rpx;
  position: relative;
  width: 100%;
}
.filter_container>view{
  height: 40rpx;
  margin-right: 40rpx;
  font-size: 25rpx;
}
.filter_container .select{
  border-bottom: 2rpx  solid #000;
  color: #000;
}
.filter_container .unselect{
  color: #9FA0A0;
}
.filter_container .question_conatiner{
  height: 40rpx;
  position: absolute;
  right: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row-reverse;
}
.question_conatiner>text{
  font-size: 28rpx;
  color: #0090FF;
  line-height: 40rpx;
}
.question_conatiner>image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 6rpx;
}
.search-box{
  width: 510rpx;
  height: 60rpx;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 28rpx 117rpx;
}
.search-box>image{
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  right: 20rpx;
  z-index: 10;
}
.search-box>input{
  width: 510rpx;
  height: 100%;
  background: #FFF;
  font-size: 28rpx;
  color: #666;
  border-radius: 28rpx;
  padding-left: 28rpx;
}


.data_container{
  width: 100%;
  height: 150rpx;
  background: #FFF;
  display: flex;
  align-items: center;
  position: relative;
  flex-direction: row;
  margin-bottom: 35rpx;
}
.data_container>image{
  width: 92rpx;
  height: 92rpx;
  border-radius: 17rpx;
  position: absolute;
  left: 25rpx;
}
.data_container .name{
  line-height: 30rpx;
  font-size: 24rpx;
  color: #000;
  position: absolute;
  left: 132rpx;
  width: 300rpx;
  overflow: hidden;
  top: 80rpx;
  display: inline-block;
  height: 30rpx;
}
.data_container .pcode{
  line-height: 30rpx;
  white-space: nowrap;
  width: 300rpx;  
  text-overflow:ellipsis; 
  overflow:hidden;
  font-size: 24rpx;
  color: #000;
  position: absolute;
  left: 132rpx;
  top: 40rpx;
}
.data_container .price{
  line-height: 30rpx;
  font-size: 24rpx;
  color: #000;
  position: absolute;
  right: 264rpx;
}
.data_container .line{
  width: 2rpx;
  height: 74rpx;
  background: #DCDDDD;
  position: absolute;
  right: 232rpx;
}
.data_container .number{
  line-height: 28rpx;
  font-size: 20rpx;
  color: #9FA0A0;
  position: absolute;
  top: 40rpx;
  right: 28rpx;
}
.data_container .code{
  line-height: 24rpx;
  font-size: 17rpx;
  width: 190rpx;
  white-space: nowrap;  
  text-overflow:ellipsis; 
  overflow:hidden;
  color: #9FA0A0;
  position: absolute;
  top: 40rpx;
  right: 28rpx;
}
.data_container .custom_price{
  line-height: 24rpx;
  font-size: 17rpx;
  color: #9FA0A0;
  position: absolute;
  top: 64rpx;
  right: 28rpx;
}
.data_container .agent_price{
  line-height: 24rpx;
  font-size: 17rpx;
  color: #9FA0A0;
  position: absolute;
  top: 86rpx;
  right: 28rpx;
}